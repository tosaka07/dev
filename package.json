{
  "name": "tosaka-dev-blog",
  "description": "a blog site.",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "sapper:dev": "sapper dev",
    "sapper:build": "sapper build --legacy",
    "sapper:export": "sapper export --legacy",
    "dev": "run-p sapper:dev watch:tailwind",
    "build": "run-s build:tailwind sapper:build",
    "export": "run-s build:tailwind sapper:export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "check": "npx svelte-check",
    "lint": "eslint . --ext .js,.svelte --fix",
    "format": "prettier --write --svelte-sort-order scripts-markup-styles ./**/*.svelte"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "compression": "^1.7.1",
    "contentful": "^7.14.6",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.7",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@tsconfig/svelte": "^1.0.2",
    "autoprefixer": "^9.8.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-check": "^0.1.55",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.5.2",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "typescript": "^3.9.7"
  }
}
